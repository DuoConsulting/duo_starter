{
    "name": "duoconsulting/duo_starter",
    "type": "drupal-profile",
    "description": "Provides a default installation profile for Duo Consulting.",
    "keywords": [
        "drupal",
        "installation",
        "profile"
    ],
    "require": {
        "drupal/address": "^1.7",
        "drupal/admin_toolbar": "^1.27",
        "drupal/adminimal_admin_toolbar": "^1.9",
        "drupal/anchor_link": "^1.6",
        "drupal/block_field": "^1.0@alpha",
        "drupal/config_filter": "^1.4",
        "drupal/config_ignore": "^2.1",
        "drupal/config_split": "^1.0.0",
        "drupal/config_update": "^1.6",
        "drupal/contact_block": "^1.4",
        "drupal/contact_storage": "^1.0@beta",
        "drupal/entity_reference_revisions": "^1.6",
        "drupal/field_group": "^3.0",
        "drupal/google_tag": "^1.1",
        "drupal/linkit": "^4.3",
        "drupal/metatag": "^1.8",
        "drupal/pathauto": "^1.4",
        "drupal/rabbit_hole": "^1.0@beta",
        "drupal/simple_sitemap": "^3.2",
        "drupal/smart_trim": "^1.1",
        "drupal/token": "^1.5",
        "drupal/vendor_stream_wrapper": "^1.3",
        "drupal/video_embed_field": "^2.1",
        "composer/installers": "~1.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "enable-patching": true,
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "docroot/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "docroot/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "drush/Commands/{$name}": [
                "type:drupal-drush"
            ]
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": []
    },
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Duo Consulting"
        }
    ]
}
