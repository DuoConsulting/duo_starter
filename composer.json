{
    "name": "duoconsulting/duo_starter",
    "type": "drupal-profile",
    "description": "Provides a default installation profile for Duo Consulting.",
    "keywords": [
        "drupal",
        "installation",
        "profile"
    ],
    "require": {
        "composer/installers": "~1.0",
        "drupal/address": "^1.7",
        "drupal/admin_toolbar": "^2.0",
        "drupal/anchor_link": "^1.6",
        "drupal/block_field": "^1.0@alpha",
        "drupal/config_filter": "^1.4",
        "drupal/config_ignore": "^2.1",
        "drupal/config_split": "^1.0",
        "drupal/config_update": "^1.6",
        "drupal/contact_block": "^1.4",
        "drupal/contact_storage": "^1.0@beta",
        "drupal/devel": "^2.0",
        "drupal/entity_reference_revisions": "^1.6",
        "drupal/environment_indicator": "^3.6",
        "drupal/field_group": "^3.0",
        "drupal/google_tag": "^1.1",
        "drupal/layout_builder_modal": "^1.0@alpha",
        "drupal/layout_builder_restrictions": "^2.2",
        "drupal/linkit": "^4.3",
        "drupal/media_library_theme_reset": "1.0-beta1",
        "drupal/metatag": "^1.8",
        "drupal/paragraphs": "^1.9",
        "drupal/pathauto": "^1.4",
        "drupal/rabbit_hole": "^1.0@beta",
        "drupal/redirect": "^1.4",
        "drupal/seckit": "^1.1",
        "drupal/simple_sitemap": "^3.2",
        "drupal/smart_trim": "^1.1",
        "drupal/stage_file_proxy": "^1.0@RC",
        "drupal/token": "^1.5",
        "drupal/toolbar_themes": "^1.0",
        "drupal/vendor_stream_wrapper": "^1.3",
        "duoconsulting/duo_admin_styles": "^8.2",
        "duoconsulting/duo_blocks": "^8.1",
        "duoconsulting/duo_foundation": "^8.1",
        "duoconsulting/duo_layout_page": "^8.1",
        "duoconsulting/duo_layouts": "^8.1"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "enable-patching": true,
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "docroot/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "docroot/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "drush/Commands/{$name}": [
                "type:drupal-drush"
            ]
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "drupal/core": {
                "Issue #3045171: Form blocks rendered inside layout builder break save": "https://www.drupal.org/files/issues/2020-02-04/layout-builder-save-issue-3045171-140.patch"
            },
            "drupal/paragraphs": {
                "Issue #2901390: Integrity constraint violation: 1048 Column 'langcode' cannot be null:": "https://www.drupal.org/files/issues/2019-07-10/paragraphs-set_langcode_widgets-2901390-29.patch"
            },
            "drupal/rabbit_hole": {
                "Issue #3066541: Do not add Rabbit Hole fields to Layout Builder form.": "https://www.drupal.org/files/issues/2019-07-08/3066541-2.patch"
            }
        }
    },
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        "duoconsulting/duo_admin_styles": {
            "url": "https://github.com/DuoConsulting/duo_admin_styles",
            "type": "git"
        },
        "duoconsulting/duo_blocks": {
            "url": "https://github.com/DuoConsulting/duo_blocks",
            "type": "git"
        },
        "duoconsulting/duo_foundation": {
            "url": "https://github.com/DuoConsulting/duo_foundation",
            "type": "git"
        },
        "duoconsulting/duo_layout_page": {
            "url": "https://github.com/DuoConsulting/duo_layout_page",
            "type": "git"
        },
        "duoconsulting/duo_layouts": {
            "url": "https://github.com/DuoConsulting/duo_layouts",
            "type": "git"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Duo Consulting"
        }
    ]
}
